<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travelers_calculator">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"

        android:icon="@mipmap/ic_logo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo2_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">


        </activity>
        <activity android:name=".toolbar.settings.SettingsToolbar"
            android:screenOrientation="portrait"
            android:label="Settings"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".toolbar.about.About"
            android:screenOrientation="portrait"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".toolbar.history.History"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name="com.example.travelers_calculator.splash.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".onboarding.Onboarding">
        </activity>

    </application>

</manifest>